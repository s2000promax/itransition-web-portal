{
  "name": "database",
  "version": "1.0.0",
  "description": "MySQL Server Deployment with Docker",
  "scripts": {
    "download": "docker pull container-registry.oracle.com/mysql/community-server:latest",
    "start:dev": "docker run --name=intransition-web-portal-container --restart on-failure -d container-registry.oracle.com/mysql/community-server",
    "start:prod": "docker run --name=intransition-web-portal-container -p 80:33060 --restart on-failure -d container-registry.oracle.com/mysql/community-server",
    "restart": "docker restart intransition-web-portal-container -p 80:33060",
    "restart:prod": "docker restart intransition-web-portal-container",
    "stop": "docker stop intransition-web-portal-container",
    "logs": "docker logs intransition-web-portal-container",
    "logs:g": "docker logs intransition-web-portal-container 2>&1 | grep GENERATED",
    "delete": "docker rm intransition-web-portal-container",
    "connect": "docker exec -it intransition-web-portal-container mysql -uroot -p",
    "docker:up": "docker compose -f docker/docker-compose.yml up -d",
    "docker:down": "docker compose -f docker/docker-compose.yml down"
},
  "keywords": ["Docker", "mySQL"],
  "author": "Stan <s2000promax@gmail.com>",
  "license": "ISC"
}
